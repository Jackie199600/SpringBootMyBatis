<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xcl.repository.BookCaseRepository">

    <select resultType="BookCase" parameterType="java.lang.Integer" id="findById">
        select * from bookcase where id = #{id}
    </select>

    <select resultType="BookCase" id="findAll">
        select * from bookcase
    </select>

</mapper>